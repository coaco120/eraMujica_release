;-------------------------------------------------
@KOJO_DATUI
;前处理
CALL KOJO_BAA(-1)
;调教者但脱但せた
IF TFLAG:强制脱衣 && TFLAG:助手ACT != 70
	;例外：主人助手（TARGET的サポート脱衣口上被呼ぶ）
	IF ASSI >= 0 && TALENT:ASSI:主人
		;助手被暂时的に调教者和交代做
		CALL ASSISWAP
		;口上色设定
		CALL KOJO_COLOR(ASSI)
		;口上显示开また是口上强制呼び出し时、口上呼び出し
		SIF FLAG:口上 || RESULT
			CALL KOJO_CALL(@"DATUI_ASSI_K{NO:ASSI}")
		;后处理
		CALL KOJO_BAA(-2)
		;助手被助手に戻す
		CALL ASSISWAP
	;通常
	ELSE
		;口上色设定
		CALL KOJO_COLOR(TARGET)
		;口上显示开また是口上强制呼び出し时、口上呼び出し
		SIF FLAG:口上 || RESULT
			CALL KOJO_CALL(@"DATUI_K{NO:TARGET}")
		;后处理
		CALL KOJO_BAA(-2)
	ENDIF
	;口上显示开那么口上但显示さ受到か被返す
	;※上记注释参照
	RETURN FLAG:口上 ? RESULT # 0
;助手但脱但せた
ELSEIF TFLAG:强制脱衣 && TFLAG:助手ACT == 70
	;右揃え
	ALIGNMENT RIGHT
	;例外：主人助手（ASSI的脱衣口上被呼ぶ）
	IF ASSI >= 0 && TALENT:ASSI:主人
		;助手被暂时的に调教者和交代做
		CALL ASSISWAP
		;口上色设定
		CALL KOJO_COLOR(TARGET)
		;口上显示开また是口上强制呼び出し时、口上呼び出し
		SIF FLAG:口上 || RESULT
			CALL KOJO_CALL(@"DATUI_K{NO:TARGET}")
		;后处理
		CALL KOJO_BAA(-2)
		;助手被助手に戻す
		CALL ASSISWAP
	;通常
	ELSE
		;口上色设定
		CALL KOJO_COLOR(ASSI)
		;口上显示开また是口上强制呼び出し时、口上呼び出し
		SIF FLAG:口上 || RESULT
			CALL KOJO_CALL(@"DATUI_ASSI_K{NO:ASSI}")
		;后处理
		CALL KOJO_BAA(-2)
	ENDIF
	;左揃えに戻す
	ALIGNMENT LEFT
ENDIF

;调教者但脱いだ
IF TFLAG:自主脱衣
	;口上色设定
	CALL KOJO_COLOR(TARGET)
	;口上显示开また是口上强制呼び出し时、口上呼び出し
	SIF FLAG:口上 || RESULT
		CALL KOJO_CALL(@"DATUI_K{NO:TARGET}")
	;后处理
	CALL KOJO_BAA(-2)
ENDIF

;口上显示开那么口上但显示さ受到か被返す
RETURN FLAG:口上 ? RESULT # 0

