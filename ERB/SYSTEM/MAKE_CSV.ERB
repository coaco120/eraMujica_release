
@MAKE_CSV

DELALLCHARA

FOR LOCAL, 0, 82
	ADDCHARA LOCAL
NEXT

$INPUT

INPUT

TARGET = RESULT

CLEARLINE LINECOUNT

PRINTL 

PRINTL ;角色番号(NO)
PRINTFORML 番号,{NO}

PRINTL 

PRINTL ;名前(NAME), 呼び名(CALLNAME)
PRINTFORML 名前,%NAME%
PRINTFORML 呼び名,%CALLNAME%

PRINTL 

PRINTL ;基礎(BASE)
FOR LOCAL, 0, VARSIZE("BASE")
	IF BASE:LOCAL != 0
		PRINTFORML 基礎,%BASENAME:LOCAL%,{BASE:LOCAL}
	ENDIF
NEXT

PRINTL 

PRINTL ;素質(TALENT)
FOR LOCAL, 0, VARSIZE("TALENT")
	IF TALENT:LOCAL == 1
		PRINTFORML 素質,%TALENTNAME:LOCAL%,
	ELSEIF TALENT:LOCAL != 0
		PRINTFORML 素質,%TALENTNAME:LOCAL%,{TALENT:LOCAL}
	ENDIF
NEXT

PRINTL 

PRINTL ;能力(ABL)
FOR LOCAL, 0, VARSIZE("ABL")
	IF ABLNAME:LOCAL != ""
		PRINTFORML 能力,%ABLNAME:LOCAL%,{ABL:LOCAL}
	ENDIF
NEXT

PRINTL 

PRINTL ;経験(EXP)
FOR LOCAL, 0, VARSIZE("EXP")
	IF EXP:LOCAL != 0
		PRINTFORML 経験,%EXPNAME:LOCAL%,{EXP:LOCAL}
	ENDIF
NEXT

PRINTL 

PRINTL ;相性(RELATION)
FOR LOCAL, 0, CHARANUM
	IF NAME:LOCAL != "" && RELATION:LOCAL != 0
		PRINTFORML %"相性," + TOSTR(LOCAL) + "," + TOSTR(RELATION:LOCAL) + ",", 15, LEFT%;%NAME:LOCAL%
	ENDIF
NEXT

PRINTL 

PRINTL ;装着物(EQUIP)
PRINTFORML %"装着物," + EQUIPNAME:0 + "," + TOSTR(EQUIP:0) + ",", 25, LEFT%;1,黑色鸭舌帽、2,墨镜、3,发带、4,华丽的黑金披肩、5,红色发带
PRINTFORML %"装着物," + EQUIPNAME:1 + "," + TOSTR(EQUIP:1) + ",", 25, LEFT%;1,短袜、2,红色长筒过膝袜、3,过膝袜、4,分趾袜
PRINTFORML %"装着物," + EQUIPNAME:2 + "," + TOSTR(EQUIP:2) + ",", 25, LEFT%;1,衬裤、2,内裤、3,短裤、4,连裤袜、5,缠腰布
PRINTFORML %"装着物," + EQUIPNAME:3 + "," + TOSTR(EQUIP:3) + ",", 25, LEFT%;1,胸罩、2,吊带背心、3,裹胸、4,Ｔ衬衫
PRINTFORML %"装着物," + EQUIPNAME:4 + "," + TOSTR(EQUIP:4) + ",", 25, LEFT%;1,长裙、2,格仔裙、3,双排扣短裤、4,裤子、5,整齐的月之森百摺裙
PRINTFORML %"装着物," + EQUIPNAME:5 + "," + TOSTR(EQUIP:5) + ",", 25, LEFT%;1,白色T衫、2,丝带蝶结外衣、3,马甲及衬衣、4,裤子、5,亮丽的月之森制服外套
PRINTFORML %"装着物," + EQUIPNAME:6 + "," + TOSTR(EQUIP:6) + ",", 25, LEFT%;1,和服、2,连衣裙、3,连衣可爱的红白色连衣裙

PRINTL 

PRINTL ;角色情报(CSTR)
FOR LOCAL, 10, 15
	IF CSTRNAME:LOCAL != ""
		PRINTFORML CSTR,%CSTRNAME:LOCAL%,%CSTR:LOCAL%
	ENDIF
NEXT

PRINTL 

PRINTL ;称呼列表(CSTR)
FOR LOCAL, 20, 22
	IF CSTRNAME:LOCAL != ""
		PRINTFORML CSTR,%CSTRNAME:LOCAL%,%CSTR:LOCAL%
	ENDIF
NEXT

PRINTL 

OUTPUTLOG

GOTO INPUT

