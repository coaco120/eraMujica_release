	CALL KOJO_EVENT(202, 1, ARG)
	PRINTFORMW %CALLNAME:ARG%从背后抱住%CALLNAME:MASTER%揉了揉胸部…
	LOCAL = 2
ENDIF
;ARG的屈服刻印4以上かつ相关做刻印被取得済である场合
{
IF MARK:ARG:屈服刻印 > 4 && 
	((PENIS(MASTER) && GETBITAND(CFLAG:ARG:刻印, 2, 3, 12, 19)) || 
	(!PENIS(MASTER) && GETBITAND(CFLAG:ARG:刻印, 3, 6, 12, 15)))
}
	PRINT [0] - 无视　
	PRINT [1] - 委身
	CALL INPUTINT(0, 1)
;条件被满たさない场合是RESULTに0被入れる
ELSE
	RESULT = 0
ENDIF
SELECTCASE RESULT
	;无视
	CASE 0
		;202, 0-2（无視したら諦めて出て行った）
		CALL KOJO_EVENT(202, 2, ARG)
		PRINTFORMW %CALLNAME:ARG%固执的爱抚着但%CALLNAME:MASTER%决定无视她于是只好放弃离开…
		;调教对象但男人的场合。そう不是场合是1/2的确率
		IF LOCAL == 1
			;MASTER的経験増加和内容的显示
			EXP:MASTER:Ｃ経験 += 1
			PRINTL Ｃ経験 +1
			;ARG的経験増加和内容的显示
			EXP:ARG:手淫経験 += 1 
			PRINTFORML （%CALLNAME:ARG%）手淫経験 +1
		;其他
		ELSE
			;MASTER的経験増加和内容的显示
			EXP:MASTER:Ｂ経験 += 1
			PRINTL Ｂ経験 +1
			;ARG的経験増加和内容的显示
			EXP:ARG:手淫経験 += 1 
			PRINTFORML （%CALLNAME:ARG%）手淫経験 +1
		ENDIF
		;平时定位的減少。"平时"但"平时定位"であ时见た只で判る人但どれ只いるだろう？ 居るわけないわそんなもんｗ
		CFLAG:ARG:平时 -= 100
		;平时定位但-999以下那么-999に调整
		SIF CFLAG:ARG:平时 < -999
			CFLAG:ARG:平时 = -999
		;ムラムラ増加
		CFLAG:ARG:起兴 += 1
		PRINTFORML （%CALLNAME:ARG%）起兴 +1
	;委身
	CASE 1
		;202 0-3（身被ゆだねて"股间/乳房"でイかされ）
		CALL KOJO_EVENT(202, 2+LOCAL, ARG)
		PRINTFORMW 当确认%CALLNAME:MASTER%不会抵抗时%CALLNAME:ARG%露出了得意的笑容将其引导向绝顶…
		;调教对象但男人的场合、男人以外的场合是1/2的确率
		IF LOCAL == 1
			;调教对象に阴茎あり
			IF PENIS(MASTER)
				;调教对象に下半身内衣あり
				IF TEQUIP:MASTER:下半身内衣
					PRINTL 
					;202, 0-5（ぱんつお着替えさ命令くれました）
					CALL KOJO_EVENT(202, 5, ARG)
					PRINTFORML %CALLNAME:ARG%脱掉了被精液沾满的%CALLNAME:MASTER%的内衣
					PRINTFORMW 换上了新的内衣…
				ENDIF
				;MASTER的経験増加和内容的显示
				EXP:MASTER:Ｃ経験 += 5
				PRINTL Ｃ経験 +5
				EXP:MASTER:绝顶経験 += 1
				PRINTL 绝顶経験 +1
				EXP:MASTER:射精経験 += 1
				PRINTL 射精経験 +1
				;ARG的経験増加和内容的显示
				EXP:ARG:精液経験 += 1 
				PRINTFORML （%CALLNAME:ARG%）精液経験 +1
				EXP:ARG:手淫経験 += 1 
				PRINTFORML （%CALLNAME:ARG%）手淫経験 +1
				EXP:ARG:高潮経験 += 1
				PRINTFORML （%CALLNAME:ARG%）高潮経験 +1
				BASE:MASTER:射精 -= 300
				;MASTER的精力0的场合
				SIF FLAG:日常控制 == 2
					TCVAR:MASTER:空射精数 += 1
			;调教对象に阴茎无
			ELSE
				;调教对象に下半身内衣あり
				IF TEQUIP:MASTER:下半身内衣
					PRINTL 
					;202, 0-5（ぱんつお着替えさ命令くれました）
					CALL KOJO_EVENT(202, 5, ARG)
					PRINTFORML %CALLNAME:ARG%脱掉了%CALLNAME:MASTER%沾满爱液的内衣
					PRINTFORMW 换上了新的内衣…
				ENDIF
				;MASTER的経験増加和内容的显示
				EXP:MASTER:Ｃ経験 += 5
				PRINTL Ｃ経験 +5
				EXP:MASTER:绝顶経験 += 1
				PRINTL 绝顶経験 +1
				;ARG的経験増加和内容的显示
				EXP:ARG:手淫経験 += 1 
				PRINTFORML （%CALLNAME:ARG%）手淫経験 +1
				EXP:ARG:高潮経験 += 1
				PRINTFORML （%CALLNAME:ARG%）高潮経験 +1
			ENDIF
		;其他
		ELSE
			;MASTER的経験増加和内容的显示
			EXP:MASTER:Ｂ経験 += 5
			PRINTL Ｂ経験 +5
			EXP:MASTER:绝顶経験 += 1
			PRINTL 绝顶経験 +1
			;ARG的経験増加和内容的显示
			EXP:ARG:手淫経験 += 1 
			PRINTFORML （%CALLNAME:ARG%）手淫経験 +1
			EXP:ARG:高潮経験 += 1
			PRINTFORML （%CALLNAME:ARG%）高潮経験 +1
		ENDIF
		;ムラムラ解消
		CFLAG:ARG:起兴 = 0
ENDSELECT
WAIT
RETURN 1

