IF TEQUIP:飞机杯 && !IS_NOWACTNAME("Ｃ道具爱抚")
	A = 150 * (2 + ITEM:9 + TALENT:道具使) / 2
	SOURCE:MASTER:快Ｃ += A
	SIF ABL:MASTER:Ｃ感觉 > 1
		SOURCE:MASTER:中毒充足 += GET_ABL(MASTER, "Ｃ感觉") * (GET_ABL(MASTER, "Ｃ感觉") - 10) * (ITEM:9 + 1) / 5 + A / 5
