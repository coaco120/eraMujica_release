		CASE 4
			CALL SET_POSE(MASTER, "坐着")
			CALL SET_POSE(TARGET, "坐着", "对面", "无效")
		;背面座位
		CASE 5
			CALL SET_POSE(MASTER, "坐着")
			CALL SET_POSE(TARGET, "坐着", "前面", "无效")
	ENDSELECT
;ACTご和的姿势情报被代入
;ただしCOM次第で是变更になる（发狂など）
ELSE
