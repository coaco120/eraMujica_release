									ELSE
										CALL SET_POSE(MASTER, "仰向")
										CALL SET_POSE(TARGET, "坐着", "对面", "无效")
									ENDIF
								;起立
								CASE 4
									;Ｃでイった直后是跪着に变化做
									IF NOWEX:MASTER:Ｃ绝顶
										CALL SET_POSE(MASTER, "膝立")
