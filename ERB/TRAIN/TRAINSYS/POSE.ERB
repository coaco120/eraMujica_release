;使用状态变更関数
;ARG:0,1是对象者1,2
;ARGS:0,1是使用状态被变更做部位1,2
;	部位     Ｐ(阴茎)/Ｖ(小穴)/Ａ(肛门)/Ｂ(胸/バスト)/Ｍ(口/マウス)/Ｈ(手/ハンド)/Ｆ(足/フット/フィート)
;-------------------------------------------------
@SET_USE(ARG:0, ARGS:0, ARG:1 = -1, ARGS:1)
#LOCALSSIZE 2
#DIM LCOUNT
FOR LCOUNT, 0, 2
	;互换性的ため的暂时处理！！！！！
	IF ARG:LCOUNT == MASTER
		SELECTCASE ARGS:LCOUNT
			CASE "Ｐ", "阴茎"
				SIF GROUPMATCH(ARGS:(!LCOUNT), "Ｖ", "小穴", "Ａ", "肛门")
					TFLAG:Ｐ插入中++
				TFLAG:Ｐ使用中++
			CASE "Ｖ", "小穴"
				TFLAG:Ｖ使用中++
			CASE "Ａ", "肛门"
				TFLAG:Ａ使用中++
			CASE "Ｂ", "胸", "バスト"
				TFLAG:胸使用中++
			CASE "Ｍ", "口", "マウス"
				TFLAG:口使用中++
			CASE "Ｈ", "手", "ハンド"
				TFLAG:手使用中++
			CASE "Ｆ", "足", "フット", "フィート"
				TFLAG:足使用中++
		ENDSELECT
	ENDIF
NEXT

;-------------------------------------------------
;姿势变更関数
;ARG是对象者
;ARGS:0是现在的姿势
;	TCVER:姿势     0=自由/1=仰向(仰面)/2=坐着/3=四這(趴着)/4=起立/10=膝立(跪着)
;ARGS:1是MASTER和的前后関係
;	TCVER:位置前后 0=无效/1=对面/2=背后(MASTER的背后)/3=前面(MASTER背对)
;ARGS:2是MASTER和的上下関係
;	TCVER:位置上下 0=无效/1=上位(上位)/2=下位(下位)
;-------------------------------------------------
@SET_POSE(ARG, ARGS:0, ARGS:1, ARGS:2)
#LOCALSSIZE 2
#DIM LCOUNT
;互换性的ため的暂时处理！！！！！
IF ARG == MASTER
	;姿势设置
	;设置不做场合是无效な字符串被指定("", "継续" など)
	SELECTCASE ARGS:0
		CASE "自由"
			TCVAR:ARG:姿势 = 0
		CASE "仰向", "仰面"
			TCVAR:ARG:姿势 = 1
			;仰面处理もついでにやっちゃう
			TFLAG:仰面 = 2
		CASE "坐着"
			TCVAR:ARG:姿势 = 2
		CASE "四這", "趴着"
			TCVAR:ARG:姿势 = 3
		CASE "起立"
			TCVAR:ARG:姿势 = 4
		CASE "膝立", "跪着"
			TCVAR:ARG:姿势 = 10
		CASE "上次"
			TCVAR:ARG:姿势 = TCVAR:ARG:上回合的姿势
	ENDSELECT
ENDIF
;互换性的ため的暂时处理！！！！！
ARG = MASTER
;前后関係设置
;设置不做场合是无效な字符串被指定("", "継续" など)
SELECTCASE ARGS:1
	CASE "无效"
		TCVAR:ARG:位置前后 = 0
	CASE "对面"
		TCVAR:ARG:位置前后 = 1
	CASE "背后", "MASTER的背后"
		TCVAR:ARG:位置前后 = 2
	CASE "前面", "MASTER背对"
		TCVAR:ARG:位置前后 = 3
	CASE "上次"
		TCVAR:ARG:位置前后 = TCVAR:ARG:上次的位置前后
ENDSELECT
;上下関係设置
;设置不做场合是无效な字符串被指定("", "継续" など)
SELECTCASE ARGS:2
	CASE "无效"
		TCVAR:ARG:位置上下 = 0
	CASE "上位", "上位"
		TCVAR:ARG:位置上下 = 1
	CASE "下位", "下位"
		TCVAR:ARG:位置上下 = 2
	CASE "上次"
		TCVAR:ARG:位置上下 = TCVAR:ARG:上次的位置上下
ENDSELECT

