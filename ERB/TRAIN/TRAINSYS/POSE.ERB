						IF TFLAG:仰面
							CALL SET_POSE(MASTER, "仰向")
							CALL SET_POSE(TARGET, "起立", "对面", "无效")
						ELSE
							;上回合的姿势によって修正
							SELECTCASE TCVAR:MASTER:上回合的姿势
								;仰面是継续
								CASE 1
									CALL SET_POSE(MASTER, "仰向")
									CALL SET_POSE(TARGET, "起立", "对面", "无效")
								;それ以外
								CASEELSE
									;Ｃでイった直后是1/2で仰面に变化做
									IF NOWEX:MASTER:Ｃ绝顶 && RAND:2
										CALL SET_POSE(MASTER, "仰向")
										CALL SET_POSE(TARGET, "起立", "对面", "无效")
									;そうでなければ座り
									ELSE
										CALL SET_POSE(MASTER, "坐着")
										CALL SET_POSE(TARGET, "坐着", "对面", "无效")
									ENDIF
							ENDSELECT
						ENDIF
					;用口している
					ELSEIF MC_PLAYER(1) == 1 || MV_PLAYER(1) == 1
						;すでに仰面的场合
						IF TFLAG:仰面
							CALL SET_POSE(MASTER, "仰向")
