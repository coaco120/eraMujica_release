									ELSE
										CALL SET_POSE(MASTER, "四這")
										CALL SET_POSE(TARGET, "膝立", "无效", "下位")
									ENDIF
								;座り
								CASE 2
									;3/4で跪着に变化做
									IF RAND:4
										CALL SET_POSE(MASTER, "膝立")
									;そうでなければ趴着
									ELSE
										CALL SET_POSE(MASTER, "四這")
										CALL SET_POSE(TARGET, "膝立", "无效", "下位")
									ENDIF
								;それ以外是趴着
								CASEELSE
									CALL SET_POSE(MASTER, "四這")
									CALL SET_POSE(TARGET, "膝立", "无效", "下位")
