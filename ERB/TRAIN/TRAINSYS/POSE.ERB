						IF TFLAG:仰面
							CALL SET_POSE(MASTER, "仰向")
							CALL SET_POSE(TARGET, "坐着", "对面", "上位")
						ELSE
							;上回合的姿势によって修正
							SELECTCASE TCVAR:MASTER:上回合的姿势
								;仰面是継续
								CASE 1
									CALL SET_POSE(MASTER, "仰向")
									CALL SET_POSE(TARGET, "坐着", "对面", "上位")
								;それ以外
								CASEELSE
									;Ｃでイった直后是1/2で仰面に变化做
									IF NOWEX:MASTER:Ｃ绝顶 && RAND:2
										CALL SET_POSE(MASTER, "仰向")
