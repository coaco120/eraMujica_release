@ASSI_ACTABLE, ARGS, ARG
#FUNCTION
SELECTCASE ARGS
	;---------------------------------
	;Ｃ爱抚
	;---------------------------------
	CASE "Ｃ爱抚"
		SIF TFLAG:Ｐ使用中
			RETURNF 0
		SIF TFLAG:Ｐ插入中
			RETURNF 0
		SIF TFLAG:插入助手中
			RETURNF 0
		SIF TEQUIP:性交奉仕中
			RETURNF 0
		SIF TEQUIP:飞机杯
			RETURNF 0
		SIF TEQUIP:假阴茎
			RETURNF 0
		SIF TEQUIP:三角木马
			RETURNF 0
		SIF TFLAG:惩罚标志
			RETURNF 0
		;旁白・口上的矛盾被回避做ため
		SIF TCVAR:MASTER:高潮
			RETURNF 0
	CASE "用口爱抚"
		SIF TEQUIP:假阴茎
			RETURNF 0
		SIF TEQUIP:震动棒
			RETURNF 0
		SIF CLOTHESTYPE(1, 1) == 2 && !TALENT:ARG:穿墙
			RETURNF 0
	CASE "素股"
		SIF TALENT:ARG:男人
			RETURNF 0
		SIF TEQUIP:震动棒
			RETURNF 0
		SIF TALENT:ARG:无足
			RETURNF 0
		SIF !TFLAG:仰面
			RETURNF 0
		SIF CLOTHESTYPE(1, 1) == 2 && !TALENT:ARG:穿墙
			RETURNF 0
		;面倒な的で弹く
		SIF TEQUIP:假阴茎
			RETURNF 0
		;润滑但不十分
		SIF PALAM:MASTER:润滑 < 75
			RETURNF 0
	CASE "进行乳交"
		SIF !PENIS(MASTER)
			RETURNF 0
		SIF TALENT:ARG:男人
			RETURNF 0
		SIF CLOTHESTYPE(1, 1) == 2 && !TALENT:ARG:穿墙
			RETURNF 0
		SIF !PENIS(MASTER)
			RETURNF 0
		SIF TEQUIP:假阴茎
			RETURNF 0
		;润滑但不十分
		SIF PALAM:MASTER:润滑 < 75
			RETURNF 0
	CASE "进行足交"
		SIF TALENT:ARG:无足
			RETURNF 0
		SIF !PENIS(MASTER)
			RETURNF 0
		;润滑但不十分
		SIF PALAM:MASTER:润滑 < 75
			RETURNF 0
	CASE "Ｃ道具"
		SIF CLOTHESTYPE(1, 1) == 2
			RETURNF 0
		SIF TEQUIP:阴蒂夹
			RETURNF 0
		SIF TEQUIP:假阴茎
			RETURNF 0
		IF TALENT:MASTER:男人 || TALENT:MASTER:扶她
			SIF ITEM:飞机杯 == 0
				RETURNF 0
		ENDIF
		IF !PENIS(MASTER)
			SIF ITEM:跳蛋 == 0
				RETURNF 0
		ENDIF
	CASE "逆强奸"
		SIF TCVAR:MASTER:勃起度 < 1000
			RETURNF 0
		SIF ABL:ARG:Ｖ感觉 < 3
			RETURNF 0
		SIF ABL:ARG:性交 < 3
			RETURNF 0
		SIF TEQUIP:震动棒
			RETURNF 0
		;コンビネーション的こ和被考え时なんらか的性交但是って时き是蹴っ和いたほう但无難
		SIF TEQUIP:性交中
			RETURNF 0
		SIF TEQUIP:性交奉仕中
			RETURNF 0
		SIF TEQUIP:助手性交中
			RETURNF 0
		SIF TEQUIP:助手性交奉仕中
			RETURNF 0
		SIF !TFLAG:仰面
			RETURNF 0
		SIF TALENT:ARG:男人
			RETURNF 0
		SIF CLOTHESTYPE(1, 1) == 2 && !TALENT:ARG:穿墙
			RETURNF 0
		SIF !PENIS(MASTER)
			RETURNF 0
	CASE "用手爱抚"
		RETURNF 1
	;---------------------------------------------------------------
	;Ａ爱抚
	;---------------------------------------------------------------
	CASE "Ａ爱抚"
		SIF TFLAG:Ａ使用中
			RETURNF 0
		SIF TEQUIP:肛门震动棒
			RETURNF 0
		SIF TEQUIP:肛门拉珠
			RETURNF 0
		SIF TEQUIP:灌肠器＋肛塞
			RETURNF 0
		SIF RANGE(TEQUIP:性交中, 3, 5)
			RETURNF 0
		SIF TEQUIP:性交奉仕中 == 6
			RETURNF 0
		SIF TEQUIP:假阴茎
			RETURNF 0
		;下半身外套に妨害
		SIF CLOTHESTYPE(1, 1) == 2 && !TALENT:ARG:穿墙
			RETURNF 0
	CASE "肛门爱抚"
		;润滑但不十分
		SIF PALAM:MASTER:润滑 < 500
			RETURNF 0
		;A経験但不十分
		SIF EXP:MASTER:Ａ経験 < 11
			RETURNF 0
		SIF TEQUIP:假阴茎
			RETURNF 0
		SIF TFLAG:惩罚标志
			RETURNF 0
	CASE "舔肛门"
		SIF !TFLAG:仰面
			RETURNF 0
		SIF TEQUIP:性交中 || TEQUIP:性交奉仕中
			RETURNF 0
		IF !TALENT:ARG:穿墙
			;调教对象的下半身内衣で强く妨害される
			SIF CLOTHESTYPE(1, 0)
				RETURNF 0
		ENDIF
		SIF TEQUIP:假阴茎
			RETURNF 0
		SIF TFLAG:惩罚标志
			RETURNF 0
	CASE "肛门跳蛋"
		SIF !ITEM:跳蛋
			RETURNF 0
		;调教对象的下半身外套で妨害される
		SIF CLOTHESTYPE(1, 1) == 2
			RETURNF 0
		SIF ABL:MASTER:Ａ感觉 < 2
			RETURNF 0
		SIF TEQUIP:假阴茎
			RETURNF 0
		SIF TFLAG:惩罚标志
			RETURNF 0
	CASE "肛门震动棒"
		SIF !ITEM:肛门震动棒
			RETURNF 0
		;调教对象的下半身外套で妨害される
		SIF CLOTHESTYPE(1, 1) == 2
			RETURNF 0
		;调教对象的下半身内衣で妨害される
		SIF CLOTHESTYPE(1, 0) == 2
			RETURNF 0
		SIF TEQUIP:假阴茎
			RETURNF 0
		;润滑但不十分
		SIF PALAM:MASTER:润滑 < 600
			RETURNF 0
		;A経験但不十分
		SIF EXP:MASTER:Ａ経験 < 11
			RETURNF 0
	;---------------------------------------------------------------
	;Ｂ爱抚
	;---------------------------------------------------------------
	CASE "Ｂ爱抚"
		SIF TFLAG:胸使用中
			RETURNF 0
		SIF TEQUIP:乳头夹
			RETURNF 0
		SIF TFLAG:惩罚标志
			RETURNF 0
	CASE "胸爱抚"
		;调教者に穿墙但なく、调教对象但男人
		IF !TALENT:ARG:穿墙 && TALENT:MASTER:男人
			;调教对象的上半身外套で妨害される
			SIF CLOTHESTYPE(1, 3) == 2
				RETURNF 0
			;调教对象的上半身内衣で妨害される
			SIF CLOTHESTYPE(1, 2) == 2
				RETURNF 0
		ENDIF
	CASE "舔乳头"
		IF !TALENT:ARG:穿墙
			SIF TEQUIP:性交中 == 4 || RANGE(TEQUIP:性交奉仕中, 4, 5)
				RETURNF 0
			;调教对象的上半身外套で强く妨害される
			SIF CLOTHESTYPE(1, 3)
				RETURNF 0
			;调教对象的上半身内衣で强く妨害される
			SIF CLOTHESTYPE(1, 2)
				RETURNF 0
		ENDIF
	CASE "玩弄乳头"
		SIF !ITEM:跳蛋
			RETURNF 0
		IF !TALENT:MASTER:男人
			;调教对象的上半身外套で妨害される
			SIF CLOTHESTYPE(1, 3) == 2
				RETURNF 0
			;调教对象的上半身内衣で妨害される
			SIF CLOTHESTYPE(1, 2) == 2
				RETURNF 0
		ENDIF
	;---------------------------------------------------------------
	;∨爱抚
	;---------------------------------------------------------------
	CASE "Ｖ爱抚"
		SIF TALENT:MASTER:处女
			RETURNF 0
		SIF TFLAG:Ｖ使用中
			RETURNF 0
		SIF TEQUIP:震动棒
			RETURNF 0
		SIF RANGE(TEQUIP:性交中, 1, 5)
			RETURNF 0
		SIF TEQUIP:假阴茎
			RETURNF 0
		SIF TEQUIP:助手性交中
			RETURNF 0
		SIF TALENT:MASTER:男人
			RETURNF 0
		;下半身外套に妨害
		SIF CLOTHESTYPE(1, 1) == 2 && !TALENT:ARG:穿墙
			RETURNF 0
	CASE "手指插入"
		;润滑但不十分
		SIF PALAM:MASTER:润滑 < 400
			RETURNF 0
		SIF TEQUIP:假阴茎
			RETURNF 0
		;経験但不十分
		SIF EXP:MASTER:Ｖ経験 < 6
			RETURNF 0
		SIF TALENT:MASTER:男人
			RETURNF 0
		SIF TFLAG:惩罚标志
			RETURNF 0
	CASE "震动棒"
		;调教对象的上半身外套で妨害される
		SIF CLOTHESTYPE(1, 3) == 2
			RETURNF 0
		;调教对象的上半身内衣で强く妨害される
		SIF CLOTHESTYPE(1, 2)
			RETURNF 0
		SIF TEQUIP:假阴茎
			RETURNF 0
		SIF !ITEM:震动棒
			RETURNF 0
		;润滑但不十分
		SIF PALAM:MASTER:润滑 < 500
			RETURNF 0
		SIF TALENT:MASTER:男人
			RETURNF 0
		;経験但不十分
		SIF EXP:MASTER:Ｖ経験 < 6
			RETURNF 0
	CASE "性交"
		;コンビネーション的こ和被考え时なんらか的性交但是って时き是蹴っ和いたほう但无難
		SIF TEQUIP:性交奉仕中
			RETURNF 0
		SIF TEQUIP:助手性交奉仕中
			RETURNF 0
		SIF TEQUIP:假阴茎
			RETURNF 0
		SIF !PENIS(ASSI)
			RETURNF 0
		;润滑但不十分
		SIF PALAM:MASTER:润滑 < 500
			RETURNF 0
		SIF TALENT:MASTER:男人
			RETURNF 0
		;経験但不十分
		SIF EXP:MASTER:Ｖ経験 < 11 && EXP:MASTER:性交経験 < 9
			RETURNF 0
		SIF TFLAG:惩罚标志
			RETURNF 0
		SIF TEQUIP:自慰中
			RETURNF 0
	;---------------------------------------------------------------
	;Ｍ爱抚
	;---------------------------------------------------------------
	CASE "Ｍ爱抚"
		SIF TFLAG:口使用中 >= 2
			RETURNF 0
		SIF TEQUIP:口球
			RETURNF 0
	CASE "接吻"
		SIF TFLAG:惩罚标志
			RETURNF 0
	CASE "顔面骑乘"
		SIF TFLAG:口使用中
			RETURNF 0
		SIF !TFLAG:仰面
			RETURNF 0
		SIF TFLAG:惩罚标志
			RETURNF 0
	CASE "舔足侍奉"
		SIF TFLAG:口使用中
			RETURNF 0
		SIF TALENT:ARG:无足
			RETURNF 0
	;---------------------------------------------------------------
	;羞耻
	;---------------------------------------------------------------
	CASE "卖弄自慰"
		SIF TFLAG:惩罚标志
			RETURNF 0
	CASE "辱骂"
	CASE "拍摄写真"
	SIF !TALENT:ARG:撮影技能
		RETURNF 0
	CASE "与调教者接吻"
		SIF TFLAG:惩罚标志
			RETURNF 0
	CASE "展露痴态"
	
	;---------------------------------------------------------------
	;施虐
	;---------------------------------------------------------------
	CASE "把头踩在脚下"
		SIF TALENT:ARG:无足
			RETURNF 0
		SIF !TFLAG:仰面
			RETURNF 0
	;---------------------------------------------------------------
	;其他
	;---------------------------------------------------------------
	CASE "脱衣"
	CASE "药品"
	CASE "待机"
ENDSELECT
RETURNF 1
