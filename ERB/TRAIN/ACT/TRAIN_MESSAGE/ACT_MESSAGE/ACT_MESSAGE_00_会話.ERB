;☆反应后☆
@TRAIN_MESSAGE2_5
PRINTL 
IF TFLAG:ACT派生 == 0                   ;服被脱いで的场合
	SELECTCASE COMGRONAME(TFLAG:REACT分类)
		CASE "消极的服从", "接受"           ;消极的服从/接受那么一枚脱ぐ
			IF (TEQUIP:MASTER:配饰 + TEQUIP:MASTER:袜子 + TEQUIP:MASTER:下半身内衣 + TEQUIP:MASTER:上半身内衣 + TEQUIP:MASTER:下半身外套 + TEQUIP:MASTER:上半身外套 + TEQUIP:MASTER:全身外套) == 0
				IF IS_COMGRONAME("接受")
					PRINTFORML %CALLNAME:MASTER%全裸着身子，因期待和兴奋而感到浑身都热了起来…
				ELSE
					PRINTFORML %CALLNAME:MASTER%无奈的脱下最后一件衣服，变成了全裸…
				ENDIF
			ELSE
				IF IS_COMGRONAME("接受")
					PRINTFORML %CALLNAME:MASTER%用缓慢的动作脱下一件衣服…
				ELSE
					PRINTFORML %CALLNAME:MASTER%只好脱下一件衣服…
				ENDIF
			ENDIF
		CASE "积极的服从", "淡然的服从"           ;积极的服从/淡然的服从那么二枚脱ぐ
			IF (TEQUIP:MASTER:配饰 + TEQUIP:MASTER:袜子 + TEQUIP:MASTER:下半身内衣 + TEQUIP:MASTER:上半身内衣 + TEQUIP:MASTER:下半身外套 + TEQUIP:MASTER:上半身外套 + TEQUIP:MASTER:全身外套) == 0
				PRINTFORML %CALLNAME:MASTER%\@ IS_COMGRONAME("积极的服从") ? 积极的 # 麻利的 \@脱去衣服，全裸了…
				PRINTFORML 因为期待和兴奋而滚烫的皮肤，已经激动的渗出汗水…
			;ELSEIF ACTION_APPLY2_5V(-1) == 2
			;	PRINTFORML %CALLNAME:MASTER%是服被二枚脱下后扔掉了…
			ELSEIF IS_COMGRONAME("积极的服从")
				PRINTFORML %CALLNAME:MASTER%积极的脱下衣服扔掉了…
			ELSE
				PRINTFORML %CALLNAME:MASTER%迅速地脱下衣服扔掉了…
			ENDIF
		CASEELSE                                  ;其他反应那么脱但ない
			IF PALAM:MASTER:害羞 > PALAM:MASTER:反抗        ;害羞先行
				PRINTFORMW %CALLNAME:MASTER%因为害羞没能按照%CALLNAME:TARGET%的命令脱掉衣服。%CALLNAME:TARGET%对此好像很不满…
			ELSE                                            ;反抗先行
				PRINTFORMW %CALLNAME:MASTER%因为讨厌没有回应%CALLNAME:TARGET%的要求。%CALLNAME:TARGET%对此好像很不满…
			ENDIF
	ENDSELECT
ELSE                                    ;服被着て的场合
	SELECTCASE COMGRONAME(TFLAG:REACT分类)
		CASE "消极的服从", "接受"           ;消极的服从/接受那么一枚着る
			PRINTFORML %CALLNAME:MASTER%按照命令穿上了一件衣服…
		CASE "积极的服从", "淡然的服从"           ;积极的服从/淡然的服从那么二枚着る
			PRINTFORML %CALLNAME:MASTER%按照命令穿上了衣服…
		CASEELSE                                  ;其他反应那么着ない
			PRINTFORMW %CALLNAME:MASTER%因为讨厌没有回应%CALLNAME:TARGET%的要求。%CALLNAME:TARGET%对此好像很不满…
	ENDSELECT
ENDIF
CALL TRAIN_MESSAGE2_5_ANIME
WAIT
;REACTION_MESSAGE被呼び出さないようにしてみる
