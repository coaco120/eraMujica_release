;☆反应后☆
@TRAIN_MESSAGE2_20
SELECTCASE COMGRONAME(TFLAG:REACT分类)
	CASE "接受"
		IF TCVAR:MASTER:高潮        ;高潮状态
			IF TCVAR:MASTER:焦虑度 > 1          ;焦虑中
				PRINTFORML 被快感麻痹了的%CALLNAME:MASTER%情不自禁的弯腰追逐着这种美妙的感受、
				PRINTFORML %CALLNAME%好像感觉很有趣似的摇晃着跳蛋…
			ELSE                                  ;焦虑终了・焦虑无し
				PRINTFORML %CALLNAME:MASTER%在强烈的快乐下\@ TALENT:MASTER:扶她 ? 死死压抑着的呻吟 # 娇滴滴的叫声 \@控制不住的提高了、
				PRINTFORML %CALLNAME%毫不在意地不断将跳蛋推到一边…
			ENDIF
		ELSE                            ;其他的场合
			SELECTCASE GET_COM_RANK()
				CASE "自然な行动"
					PRINTFORML %CALLNAME:MASTER%被玩具玩弄着敏感的突起成为了这种莫名其妙的快感的俘虏
					PRINTFORML 发出了下流的娇声…
				CASE "无理的ない行动"
					PRINTFORML %CALLNAME:MASTER%按照要求抬起腰沉溺在无机物的快感中…
				CASE "无理被した行动"
					PRINTFORML %CALLNAME:MASTER%被奇妙的玩具玩弄着因羞耻紧咬着嘴唇、
					PRINTFORML 强烈的快感使表情瞬间荡漾…
			ENDSELECT
		ENDIF
		RETURN 2
	CASE "积极的服从", "淡然的服从"
		SELECTCASE GET_COM_RANK()
			CASE "自然な行动"
				PRINTFORML 为了不成为%CALLNAME%的阻碍，%CALLNAME:MASTER%自己控制住了已经迫不及待的腰部…
			CASE "无理的ない行动"
				PRINTFORML 一边用奇妙的玩具执拗地爱抚着%CALLNAME:MASTER%
				PRINTFORML 按照%CALLNAME%说的那样行动，老实地张开了腿…
			CASE "无理被した行动"
				IF COMCOR_POSI()                  ;实是感受いい
					PRINTFORML %CALLNAME%一边用玩具责备着%CALLNAME:MASTER%、
					PRINTFORML 忍住了几乎使腰部沉溺其中快要崩溃的快感…
				ELSE                              ;实是嫌
					PRINTFORML %CALLNAME:MASTER%拼命地从意识中驱赶着股间涌出的快感
					PRINTFORML 按照%CALLNAME%所说的舞动起来…
				ENDIF
		ENDSELECT
		RETURN 2
	CASE "请求宽恕"
		SELECTCASE GET_COM_RANK()
			CASE "自然な行动"
				PRINTFORML %CALLNAME:MASTER%有些害怕那些不认识的玩具和它带来的感觉、
				PRINTFORML 身体因为叫人帮忙而摔倒了…
			CASE "无理的ない行动"
				PRINTFORML %CALLNAME:MASTER%有些害怕玩具强制给予的快感…
			CASE "无理被した行动"
				IF COMCOR_POSI()                  ;实是感受いい
					PRINTFORML %CALLNAME:MASTER%害怕被这些粗糙的玩具给玩弄得舒服起来、
					PRINTFORML 把哀求的视线转向了%CALLNAME%…
				ELSE                              ;实是嫌
					PRINTFORML %CALLNAME:MASTER%有点害怕%CALLNAME%手中的跳蛋所带来的快乐、
					PRINTFORML 小声说了句住手…
				ENDIF
		ENDSELECT
		RETURN 2
	CASE "消极的服从"
		SELECTCASE GET_COM_RANK()
			CASE "自然な行动"
				PRINTFORML %CALLNAME:MASTER%有些想要跳蛋的振动所强制给予的有点痒的快感了、
				PRINTFORML 一边用喉咙和鼻子小声地叫唤着，一边拼命忍耐…
			CASE "无理的ない行动"
				PRINTFORML %CALLNAME:MASTER%染红了脸，忍耐着被强制给予的快感…
			CASE "无理被した行动"
				IF COMCOR_POSI()                  ;实是感受いい
					PRINTFORML %CALLNAME:MASTER%因这样简单的玩具也能使自己感受到快感而感到后悔、
					PRINTFORML 咬紧牙关朝着%CALLNAME%的方向看去…
				ELSE                              ;实是嫌
					PRINTFORML %CALLNAME:MASTER%惊讶于%CALLNAME%手中的跳蛋所带来的快乐、
					PRINTFORML 拼命忍耐，但还是不由得呻吟了起来…
				ENDIF
		ENDSELECT
		RETURN 2
ENDSELECT

