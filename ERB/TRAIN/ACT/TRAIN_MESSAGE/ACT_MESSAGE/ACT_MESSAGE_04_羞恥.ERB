;────────────────────────────────────
;040,自慰
;────────────────────────────────────
;★反应前★
@TRAIN_MESSAGE_40
PRINTFORML %CALLNAME:TARGET%在%TARGET_DESCRIPTION("保持、")%%CALLNAME:MASTER%
PRINTFORML 命令展现出自己的动作…

;☆反应后☆
@TRAIN_MESSAGE2_40
;反应但容纳
IF IS_COMGRONAME("消极的服从/淡然的服从/积极的服从/接受")
	IF ME_PLAYER(1) == 9                ;顔射自慰绝顶
		PRINTFORML %CALLNAME:MASTER%一边捋着自己的阴茎一边告诉对方马上要射了、
		PRINTFORML %CALLNAME:TARGET%是命令在马眼前伸出舌头，让其直到飞进嘴里…
		RETURN 2
	ELSEIF IS_COMGRONAME("积极的服从/接受")
		IF TCVAR:MASTER:高潮 && TCVAR:MASTER:焦虑度 < 2         ;高潮状态で焦虑终了/无し
			IF PENIS(MASTER)                      ;调教对象に阴茎あり
				PRINTFORML %CALLNAME:MASTER%一边捋着自己的阴茎一边告诉对方马上要射了、
			ELSEIF TEQUIP:假阴茎            ;调教对象但穿上假阴茎中
				PRINTFORML %CALLNAME:MASTER%在假阴茎的底部刺激疯狂阴核的同时告诉说很快就要绝顶了、
			ELSE                                  ;其他
				PRINTFORML %CALLNAME:MASTER%玩弄自己阴核的同时告诉对方快要绝顶了、
			ENDIF
			PRINTFORML %CALLNAME:TARGET%一边低声说着着下流的话一人でイくように命じた…
		ELSEIF TEQUIP:假阴茎                                    ;调教对象但穿上假阴茎中
			PRINTFORML %CALLNAME:MASTER%握着假阴茎，像男人那样自慰起来…
		ELSE                                                          ;其他
			PRINTFORML %CALLNAME:MASTER%兴奋地看着%CALLNAME:TARGET%的视线的同时自慰起来…
		ENDIF
		RETURN 2
	ELSEIF IS_COMGRONAME("淡然的服从")
		SELECTCASE GET_COM_RANK()
			CASE "自然な行动"
				PRINTFORML %CALLNAME:MASTER%若无其事地自慰着…
			CASE "无理的ない行动"
				PRINTFORML %CALLNAME:MASTER%完全不在意%CALLNAME%的视线、
				PRINTFORML 开始尽可能地像往常一样自慰…
			CASE "无理被した行动"
				IF COMCOR_POSI()                  ;实是まんざらでもない
					PRINTFORML %CALLNAME:MASTER%一边为了不让别人看到自己的感受而努力忍耐着声音和表情、
					PRINTFORML 装做和平时一样开始自慰…
				ELSE                              ;实是嫌
					PRINTFORML 拼命忍受着被%CALLNAME%侵犯的耻辱的同时、
					PRINTFORML %CALLNAME:MASTER%若无其事的自慰了…
				ENDIF
		ENDSELECT
		RETURN 2
	ELSEIF IS_COMGRONAME("消极的服从")
		SELECTCASE GET_COM_RANK()
			CASE "自然な行动"
				PRINTFORML %CALLNAME:MASTER%虽然感到羞耻，但却无法违抗%CALLNAME%
				PRINTFORML 没办法只好开始自慰了…
			CASE "无理的ない行动"
				PRINTFORML %CALLNAME:TARGET%一直凝视着满脸羞耻地自慰着的%CALLNAME:MASTER%…
			CASE "无理被した行动"
				IF COMCOR_POSI()                  ;实是感受いい
					PRINTFORML %CALLNAME:MASTER%强忍着害羞的样子开始自慰了、
					PRINTFORML 用自己的手指得到了快乐，拼命隐藏着淫乱的表情…
				ELSE                              ;实是嫌
					PRINTFORML 被看到手淫时的羞耻使得%CALLNAME:MASTER%的脸一下子变得通红、
					PRINTFORML 当%CALLNAME%开始催促时，才勉勉强强地开始了自慰…
				ENDIF
		ENDSELECT
		RETURN 2
	ENDIF
;反应但拒绝
ELSE
	IF PALAM:MASTER:害羞 > PALAM:MASTER:反抗      ;害羞先行
		PRINTFORML %CALLNAME:MASTER%由于害羞无法按照%CALLNAME:TARGET%的命令进行自慰，对此%CALLNAME:TARGET%似乎很不满…
	ELSE                                          ;反抗先行
		PRINTFORML %CALLNAME:MASTER%因为讨厌没有回应%CALLNAME:TARGET%的要求。%CALLNAME:TARGET%对此好像很不满…
	ENDIF
	;拒绝扱いである那么ばRESULT是1で良い気但做的で变えてみる@/L
	RETURN 1
ENDIF

