;キャンセル履历管理
@CANCEL_TEQUIP_COUNT(ARGS = "", ARGS:1 = "")
#FUNCTION
IF ARGS != ""
	IF ARGS:1 == "发生"
		IF CFLAG:@"%ARGS%取消修正" <= 33
			CFLAG:@"%ARGS%取消修正" = 100
		ELSE
			CFLAG:@"%ARGS%取消修正" *= 3
		ENDIF
	ELSE
		RETURNF CFLAG:@"%ARGS%取消修正"
	ENDIF
ELSE
	SPLIT "捆绑/三角木马/野外PLAY/大镜/COSPLAY", "/", LOCALS
	FOR LOCAL, 0, RESULT
		CFLAG:@"%LOCALS:LOCAL%取消修正" -= 5 + CFLAG:@"%LOCALS:LOCAL%取消修正" / 100
	NEXT
ENDIF
RETURNF 0

