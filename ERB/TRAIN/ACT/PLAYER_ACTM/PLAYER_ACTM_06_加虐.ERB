	CALL ADD_CRI_VAR("绳", LINE_CALC(PALAM:MASTER:反抗, "0→0,5→5,LIMIT/0～5"), LINE_CALC(PALAM:MASTER:反抗, "0→0,5→2,LIMIT/0～2"))
