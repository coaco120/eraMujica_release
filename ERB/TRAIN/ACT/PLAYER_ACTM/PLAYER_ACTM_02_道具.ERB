;────────────────────────────────────
;修正値計算
;────────────────────────────────────
@ACT_ACTM2_CORRECTION(ARG)
#DIM LCOUNT

;ARG但0以外那么修正値被計算し、以后的处理被行わない
IF ARG
	;68に関して是ここで計算不做
	FOR LCOUNT, 20, 30
		CALL ACT_M_RES_CALC(LCOUNT, GET_CRI_VAR(GET_ACTNAME(LCOUNT) + "累计"))
	NEXT
ELSE
	;修正値被判定値に加算做
	CALL ACT_ACTM_CORRECTION(2)
ENDIF

