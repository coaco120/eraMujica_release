
;-----------------------------------------------------------
;ACTABLE一括管理
;-----------------------------------------------------------
@GLOBAL_ACTABLE(ARG)
#FUNCTION
#LOCALSIZE 200
#DIM 参照回合, 200
#DIM DAYTIME管理, 2

;日时更新
IF DAYTIME管理 != TIME || DAYTIME管理:1 != DAY
	VARSET 参照回合, -1
	DAYTIME管理 = TIME
	DAYTIME管理:1 = DAY
ENDIF

;回合内初回
IF 参照回合:ARG != TFLAG:调教时间
	参照回合:ARG = TFLAG:调教时间
	IF PERCENT(CFLAG:(1000+ARG))
		LOCAL:ARG = 1
	ELSE
		LOCAL:ARG = 0
	ENDIF
ENDIF

;CFLAG:1000～1199根据ACT封印判定
ACTABLE_REASON:ARG = 封印_ACT封印
IF LOCAL:ARG == 1
	;採用不可
	RESULT = 0
	RETURNF 1
ENDIF

;休息系ACT封印
;SANDBOX限定处理
ACTABLE_REASON:ARG = 封印_SANDBOX休息封印
IF FLAG:模式 == 1 && GROUPMATCH(ARG, 90, 91, 92)
	RESULT = 0
	RETURNF 1
ENDIF

